@model ITWorkLogs.Models.WorkLogs

<div class="modal-header">
    <button type="button" class="close" data-dismiss="modal" aria-hidden="true">×</button>
    <h3 class="modal-title">Task detail</h3>
</div>

<div class="modal-body">
    <div class="form-horizontal">

        <div class="form-group">
            @Html.LabelFor(m => Model.DateIN, new { @class = "control-label col-sm-3" })
            <div class="col-sm-9">
                @Html.TextBoxFor(m => m.DateIN, new { @class = "form-control required", @disabled = "disabled" })
            </div>
        </div>

        <div class="form-group">
            @Html.LabelFor(m => Model.Personnel, new { @class = "control-label col-sm-3" })
            <div class="col-sm-9">
                @Html.TextBoxFor(m => m.Personnel, new { @class = "form-control required", @disabled = "disabled" })
            </div>
        </div>

        <div class="form-group">
            @Html.LabelFor(m => Model.Department, new { @class = "control-label col-sm-3" })
            <div class="col-sm-9">
                @Html.TextBoxFor(m => m.Department, new { @class = "form-control required", @disabled = "disabled" })
            </div>
        </div>

        <div class="form-group">
            @Html.LabelFor(m => Model.PersonConcern, new { @class = "control-label col-sm-3" })
            <div class="col-sm-9">
                @Html.TextBoxFor(m => m.PersonConcern, new { @class = "form-control required", @disabled = "disabled" })
            </div>
        </div>

        <div class="form-group">
            @Html.LabelFor(m => Model.Concern, new { @class = "control-label col-sm-3" })
            <div class="col-sm-9">
                @Html.TextAreaFor(m => m.Concern, new { @class = "form-control required", @disabled = "disabled" })
            </div>
        </div>
        <div class="form-group">
            @Html.LabelFor(m => Model.Details, new { @class = "control-label col-sm-3" })
            <div class="col-sm-9">
                @Html.TextAreaFor(m => m.Details, new { @class = "form-control required", @disabled = "disabled" })
            </div>
        </div>

        <div class="form-group">
            @Html.LabelFor(m => Model.Status, new { @class = "control-label col-sm-3" })
            <div class="col-sm-9">
                @Html.TextBoxFor(m => m.Status, new { @class = "form-control required", @disabled = "disabled" })
            </div>
        </div>

        <div class="form-group">
            @Html.LabelFor(m => Model.Remarks, new { @class = "control-label col-sm-3" })
            <div class="col-sm-9">
                @Html.TextAreaFor(m => m.Remarks, new { @class = "form-control required", @disabled = "disabled" })
            </div>
        </div>

        <div class="form-group">
            @Html.LabelFor(m => Model.DateDone, new { @class = "control-label col-sm-3" })
            <div class="col-sm-9">
                @Html.TextBoxFor(m => m.DateDone, new { @class = "form-control required", @disabled = "disabled" })
            </div>
        </div>

        <div class="form-group">
            @Html.LabelFor(m => Model.DoneBy, new { @class = "control-label col-sm-3" })
            <div class="col-sm-9">
                @Html.TextBoxFor(m => m.DoneBy, new { @class = "form-control required", @disabled = "disabled" })
            </div>
        </div>
        <div class="form-group">
            @Html.LabelFor(m => Model.CreatedBy, new { @class = "control-label col-sm-3" })
            <div class="col-sm-9">
                @Html.TextBoxFor(m => m.CreatedBy, new { @class = "form-control required", @disabled = "disabled" })
            </div>
        </div>

    </div>
</div>

<div class="modal-footer">
    <button class="btn btn-warning" data-dismiss="modal">Close</button>
</div>
